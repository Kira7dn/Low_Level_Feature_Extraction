{
    "app_name": "Low_Level_Feature_Extraction",
    "environment": "development",
    "debug": true,
    "logging": {
        "level": "INFO",
        "file": "logs/app.log"
    },
    "database": {
        "host": "localhost",
        "port": 5432,
        "name": "feature_extraction"
    },
    "secrets": {
        "database_password": null,
        "api_key": null
    },
    "image_processing": {
        "max_upload_size": 10485760,
        "allowed_formats": ["png", "jpg", "jpeg", "webp"],
        "default_quality": 85
    },
    "performance": {
        "cache_enabled": true,
        "cache_ttl": 3600
    }
}
